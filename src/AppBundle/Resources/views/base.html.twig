		        '@AppBundle/Resources/public/assets/js/jquery.scrollgress.min.js'
		        '@AppBundle/Resources/public/assets/js/skel.min.js'
		        '@AppBundle/Resources/public/assets/js/util.js'
		        '@AppBundle/Resources/public/assets/js/main.js' %}
		        <script src="{{ asset_url }}"></script>
			{% endjavascripts %}

			<!--[if lte IE 8]><script src="/bundles/app/assets/js/ie/respond.min.js"></script><![endif]-->
		    <!--[if lte IE 8]><script src="/bundles/app/assets/js/ie/html5shiv.js"></script><![endif]-->

			{% stylesheets 'bundles/app/assets/css/main.css'
